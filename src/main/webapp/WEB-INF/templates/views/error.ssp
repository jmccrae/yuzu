<%@ var relPath: String %>
<%@ var layout: String %>
<%@ var error_title: String %>
<%@ var error_msg: String %>
<% attributes("layout") = layout %>
<% attributes("relPath") = relPath %>
<html>
    <head>
        <title>Downloads</title>
    </head>
    <body>
        <div class="container">
            <div class="row">
                <h1>Error: ${error_title}</h1>

                <div>${error_msg}</div>
            </div>
        </div>
    </body>
</html>
