<div class="rdf_header">
<h1 style="word-wrap:break-word;display:inline;">{{display}}</h1>
<!--<img src="{{context}}/assets/rdf_w3c_icon.48.gif" height="28px" onclick="toggle_rdf_format_list();" style="float:right;"/></h1>-->
<ul class="nav nav-tabs pull-right">
    <li class="active">
        <a href="{{uri}}.html">HTML</a></li>
    <li>
        <a href="{{uri}}.rdf">RDF/XML</a></li>
    <li>
        <a href="{{uri}}.nt">N-Triples</a></li>
    <li>
        <a href="{{uri}}.ttl">Turtle</a></li>
    <li>
        <a href="{{uri}}.json">JSON-LD</a></li>
</ul>
</div>
{{#classOf}}
    <h5 class="rdf_classof">Instance of: <a href="{{uri}}">{{display}}</a></h5>
{{/classOf}}
<h3 class="rdf_subheader">Term</h3>
<table class="rdf_table" resource="">
    {{#triples}}
        {{^has_triples}}
            {{> triple}}
        {{/has_triples}}
    {{/triples}}
    {{#inverses}}
    <tr>
        <td class="rdf_prop rdf_first_value">
            {{#prop}}
                Is <a href="{{uri}}">{{display}}</a> of
            {{/prop}}
        </td>
        <td class="rdf_value">
        {{#obj}}
                {{#elem}}
                    <a href="{{uri}}" property="{{#prop}}{{uri}}{{/prop}}" resource="">{{display}}</a>
                {{/elem}}
            {{^last}}
    </td>
</tr>
<tr>
    <td></td>
    <td class="rdf_value rdf_rest_value">
            {{/last}}
        {{/obj}}
    </td>
</tr>                
    {{/inverses}}
        
</table>

{{#triples}}
    {{#has_triples}}
        {{#prop}}
            <h3 class="rdf_subheader"><a href="{{uri}}">{{display}}</a></h3>
        {{/prop}}
            {{#obj}}
            {{#elem}}
                {{#classOf}}<h5 class="rdf_classof">Instance of: <a href="{{uri}}">{{display}}</a></h5>{{/classOf}}
            {{/elem}}
        <span property="{{#prop}}{{uri}}{{/prop}}" resource="{{#elem}}{{uri}}{{/elem}}"></span>
        <a class="anchor" id="{{#elem}}{{fragment}}{{/elem}}"></a>
        <table class="rdf_table" resource="{{#elem}}{{uri}}{{/elem}}">
                {{#elem}}
                    {{#triples}}
                        {{> triple}}
                    {{/triples}}
                {{/elem}}
        </table>
            {{/obj}}
    {{/has_triples}}
{{/triples}}
            
